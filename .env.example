# Aspayr Mobile Environment Variables
# Copy this file to .env and fill in your values

# Backend API Configuration (Required)
API_BASE_URL=https://your-backend-api-url.com

# Microsoft Authentication (Optional - for SSO)
MSAL_CLIENT_ID=your-azure-ad-client-id
MSAL_AUTHORITY=https://login.microsoftonline.com/common

# Optional: Debug mode
DEBUG=true
